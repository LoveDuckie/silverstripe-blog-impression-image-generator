{
    "name": "loveduckie/silverstripe-blog-impression-image-generator",
    "description": "A SilverStripe extension for automagically generating impression images for technical blog posts.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/LoveDuckie/silverstripe-blog-impression-image-generator",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LoveDuckie\\SilverStripe\\BlogImpressionImageGenerator\\": "src/",
            "LoveDuckie\\SilverStripe\\BlogImpressionImageGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
